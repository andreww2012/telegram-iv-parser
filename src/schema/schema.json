{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Site",
  "definitions": {
    "Site": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "options": {
          "$ref": "#/definitions/Options"
        },
        "stats": {
          "$ref": "#/definitions/Stats"
        },
        "pages": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/PageElement"
          }
        },
        "parsingResults": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ParsingResult"
          }
        },
        "articles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        }
      },
      "required": [
        "articles",
        "options",
        "pages",
        "parsingResults",
        "stats"
      ],
      "title": "Site"
    },
    "Article": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "hash": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "fromPage": {
          "type": "integer"
        },
        "code": {
          "type": "integer"
        },
        "parsed": {
          "type": "boolean"
        },
        "checked": {
          "type": "boolean"
        },
        "d": {
          "type": "integer"
        }
      },
      "required": [
        "checked",
        "code",
        "d",
        "fromPage",
        "hash",
        "parsed",
        "url"
      ],
      "title": "Article"
    },
    "Options": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "host": {
          "type": "string"
        },
        "subdomain": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "httpOnly": {
          "type": "boolean"
        },
        "section": {
          "$ref": "#/definitions/Section"
        },
        "pagination": {
          "$ref": "#/definitions/Pagination"
        },
        "page": {
          "$ref": "#/definitions/OptionsPage"
        }
      },
      "required": [
        "host",
        "name",
        "page",
        "pagination",
        "section"
      ],
      "title": "Options"
    },
    "OptionsPage": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "articleBodySelector": {
          "type": "string"
        },
        "ignoreSelectors": {
          "type": "array",
          "items": {}
        }
      },
      "required": [
        "articleBodySelector",
        "ignoreSelectors"
      ],
      "title": "OptionsPage"
    },
    "Pagination": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "reversed": {
          "type": "boolean"
        },
        "enabled": {
          "type": "boolean"
        },
        "step": {
          "type": "integer",
          "minimum": 1
        }
      },
      "required": [
        "enabled"
      ],
      "title": "Pagination"
    },
    "Section": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "pagePattern": {
          "type": "string"
        },
        "firstPageUrl": {
          "type": "string"
        },
        "linkSelector": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "array",
              "items": {}
            }
          ]
        }
      },
      "required": [
        "linkSelector",
        "pagePattern"
      ],
      "title": "Section"
    },
    "PageElement": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "num": {
          "type": "integer"
        },
        "art": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "d": {
          "type": "integer"
        }
      },
      "required": [
        "art",
        "d",
        "num"
      ],
      "title": "PageElement"
    },
    "ParsingResult": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "cat": {
          "type": "string"
        },
        "info": {
          "$ref": "#/definitions/Info"
        },
        "name": {
          "type": "string"
        },
        "count": {
          "type": "integer"
        },
        "art": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "art",
        "cat",
        "count",
        "info",
        "name"
      ],
      "title": "ParsingResult"
    },
    "Info": {
      "type": "object",
      "additionalProperties": true,
      "title": "Info"
    },
    "Stats": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "pagesCount": {
          "type": "integer"
        },
        "fileGeneratedDate": {
          "type": "integer"
        },
        "d": {
          "type": "integer"
        }
      },
      "required": [
        "d",
        "fileGeneratedDate",
        "pagesCount"
      ],
      "title": "Stats"
    }
  }
}
